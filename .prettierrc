{
	"$schema": "https://json.schemastore.org/prettierrc",
	"semi": true,
	"tabWidth": 2,
	"printWidth": 100,
	"singleQuote": false,
	"trailingComma": "all",
	"plugins": ["prettier-plugin-svelte"],
	"singleAttributePerLine": true,
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
